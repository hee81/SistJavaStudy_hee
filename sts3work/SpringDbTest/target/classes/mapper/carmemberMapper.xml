<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="db.data.carmember.CarMemberDao">
	<select id="getTotalCountOfCarMem" resultType="int">
		select count(*) from carmember
	</select>
	<insert id="insertOfCarMem" parameterType="cmdto">
		insert into carmember (name,hp,addr,gaipday) values (#{name},#{hp},#{addr},now())
	</insert>
	<select id="getAllMembersOfCarMem" resultType="cmdto">
		select * from carmember order by num desc
	</select>
	<select id="getOneMemberOfCarMem" parameterType="String" resultType="cmdto">
		select * from carmember where num=#{num}
	</select>
	<delete id="deleteOfCarMem" parameterType="String">
		delete from carmember where num=#{num}
	</delete>
	<update id="updateOfCarMem" parameterType="cmdto">
		update carmember set name=#{name},hp=#{hp},addr=#{addr} where num=#{num}
	</update>
</mapper>