<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="boot.data.mapper.IpgoMapperInter">
	<select id="getTotalCount" resultType="int">
		select count(*) from ipgo
	</select>
	<insert id="insertIpgo" parameterType="ipgodto">
		insert into ipgo (sangpum,price,photoname,ipgoday) 
		values (#{sangpum},#{price},#{photoname},now())
	</insert>
	<select id="getAllSangpums" resultType="ipgodto">
		select * from ipgo order by num
	</select>
</mapper>